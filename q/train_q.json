{
    "block_size": 9,
    "vocab_size": 10000,
    "n_layer": 2,
    "n_head": 2,
    "n_embd": 32,
    "dropout": 0.0,
    "bias": false,
    "is_causal": true,

    "learning_rate": 6e-4,
    "max_iters": 100000,
    "weight_decay": 1e-1,
    "beta1": 0.9,
    "beta2": 0.99,

    "decay_lr": true,
    "warmup_iters": 300,
    "lr_decay_iters": 100000,
    "min_lr": 6e-5,

    "init_from": "pretrain",
    "batch_size": 300,
    "random_seed": 114514,
    "eval_only_last_token_loss": true,
    "data_dir": "data_q",
    "ckpt_dir": "ckpt_q",
    "eval_interval": 100,
    "log_interval": 10,
    "eval_iters": 5,

    "backend": "nccl",
    "device": "cuda:0"
}
